{
  "miscellaneous_settings": {
    "random_seed": 42,
    "num_workers": 4,
    "detect_anomaly": false,
    "shard_size": 2048,
    "torch_compile": true,
    "compile_mode": "default",
    "torch_export": true
  },
  "data_paths_config": {
    "hdf5_dataset_filename": [
      "picaso_results_1.h5",
      "picaso_results_2.h5"
    ],
    "dataset_splits_filename": "dataset_splits.json"
  },
  "data_specification": {
    "input_variables": [
      "pressure_bar",
      "temperature_k"
    ],
    "global_variables": [
      "orbital_distance_m"
    ],
    "target_variables": [
      "net_thermal_flux",
      "net_reflected_flux"
    ],
    "padding_value": -9999.0
  },
  "normalization": {
    "key_methods": {
      "pressure_bar": "log-min-max",
      "temperature_k": "standard",
      "orbital_distance_m": "log-standard",
      "net_thermal_flux": "symlog",
      "net_reflected_flux": "symlog"
    },
    "default_method": "standard",
    "epsilon": 1e-09,
    "quantile_max_values_in_memory": 10000000,
    "symlog_percentile": 0.25
  },
  "model_hyperparameters": {
    "d_model": 256,
    "nhead": 4,
    "num_encoder_layers": 3,
    "dim_feedforward": 1024,
    "dropout": 0.0,
    "max_sequence_length": 256
  },
  "training_hyperparameters": {
    "epochs": 100,
    "batch_size": 512,
    "learning_rate": 0.0001,
    "weight_decay": 1e-05,
    "optimizer": "adamw",
    "gradient_clip_val": 1.0,
    "gradient_accumulation_steps": 1,
    "scheduler_type": "reduce_on_plateau",
    "lr_patience": 3,
    "lr_factor": 0.5,
    "warmup_epochs": 0,
    "min_lr": 1e-08,
    "early_stopping_patience": 20,
    "min_delta": 1e-08,
    "use_amp": true,
    "dataset_fraction_to_use": 1.0,
    "val_frac": 0.15,
    "test_frac": 0.15,
    "max_batch_failure_rate": 0.1
  },
  "output_paths_config": {
    "fixed_model_foldername": "trained_model"
  },
  "hyperparameter_search": {
    "d_model": [
      128,
      256,
      512
    ],
    "nhead_divisors": [
      4,
      8,
      16
    ],
    "num_encoder_layers": [
      3,
      8
    ],
    "dim_feedforward": [
      512,
      1024,
      2048
    ],
    "dropout": {
      "low": 0.0,
      "high": 0.1
    },
    "learning_rate": {
      "low": 0.0001,
      "high": 0.0005,
      "log": true
    },
    "weight_decay": {
      "low": 1e-06,
      "high": 0.0001,
      "log": true
    },
    "batch_size": [
      128,
      256,
      512
    ]
  }
}